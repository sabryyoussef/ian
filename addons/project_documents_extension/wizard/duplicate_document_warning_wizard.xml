<?xml version="1.0" encoding="utf-8"?>
<odoo>
    <data>
        <!-- Duplicate Document Warning Wizard Form View -->
        <record id="view_duplicate_document_warning_wizard_form" model="ir.ui.view">
            <field name="name">duplicate.document.warning.wizard.form</field>
            <field name="model">duplicate.document.warning.wizard</field>
            <field name="arch" type="xml">
                <form string="Duplicate Document Warning">
                    <group>
                        <field name="message" widget="text" readonly="1"/>
                    </group>
                    <footer>
                        <button name="action_close" string="OK" type="object" class="btn-primary"/>
                    </footer>
                </form>
            </field>
        </record>

        <!-- Duplicate Document Warning Wizard Action -->
        <record id="action_duplicate_document_warning_wizard" model="ir.actions.act_window">
            <field name="name">Duplicate Document Warning</field>
            <field name="res_model">duplicate.document.warning.wizard</field>
            <field name="view_mode">form</field>
            <field name="target">new</field>
        </record>
    </data>
</odoo> 